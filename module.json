  "authors": [
    {
      "name": "admiraladmirable",
      "discord": "aedif"
    }
  ],
  "bugs": "https://github.com/admiraladmirable/TokenVariants/issues",
  "socket": true,
  "esmodules": ["token-variants/bundle.js"],
  "scripts": [],
  "styles": ["token-variants/styles/tva-styles.css"],
  "languages": [
    {
